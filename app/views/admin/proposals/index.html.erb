<div class="row">

<div class="col-lg-12">
<h2 class="page-header">PROPOSTE INSERITE</h2>
<div class="table-responsive">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Titolo</th>
      <th>Tematica</th>
      <th>Data Inserimento</th>
      <th>Nome</th>
      <th>Cognome</th>

      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @proposals.each do |proposal| %>
      <tr>
        <td><%=link_to proposal.title, admin_proposal_path(proposal)%></td>
        <td><%= proposal.category.name %></td>
        <td><%= proposal.created_at.strftime("%d/%m/%Y %H:%M") %></td>
        <td><%= proposal.referent.name%></td>
        <td><%= proposal.referent.surname%></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
</div>
</div>
